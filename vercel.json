{
  "functions": {
    "api/create-checkout-session.js": {
      "maxDuration": 10
    },
    "api/retrieve-session.js": {
      "maxDuration": 10
    },
    "api/upload-files.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}